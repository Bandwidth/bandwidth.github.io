            "js": "js/custom.js"
        },
        "layout": {
            "headerPath" : "layouts/header.html",
            "footerPath" : "layouts/footer.html"
        },
        "gtm": {
            "token": "GTM-P2JT7Z",
            "virtualPageViews": true
        },
